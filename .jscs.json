{
	"maximumLineLength": {
		"value": 100,
		"tabSize": 4,
		"allowUrlComments": true,
		"allowRegex": true
	}
	,"requireSemicolons": true
	,"validateLineBreaks": "LF"
	,"validateIndentation": "\t"
	,"requireLineFeedAtFileEnd": true
	,"validateQuoteMarks": { "mark": "'", "escape": true }
	,"disallowMultipleLineStrings": true
	,"disallowMultipleSpaces": true
	,"disallowMixedSpacesAndTabs": "smart"
	,"disallowTrailingWhitespace": true
	,"disallowKeywords": ["with"]
	,"requireCurlyBraces": ["try", "catch", "do"]
	,"requireKeywordsOnNewLine": ["else", "catch", "return"]
	,"safeContextKeyword": ["module"]
	,"requireSpaceAfterKeywords": [
		"if", "else", "for", "while", "do", "switch", "return", "try", "catch", "case", "void", "return", "typeof"
	]
	,"requireParenthesesAroundIIFE": true
	,"disallowEmptyBlocks": true
	,"disallowKeywordsInComments": true
	,"disallowYodaConditions": true
	,"disallowFunctionDeclarations": true
	,"disallowSpacesInCallExpression": true
	,"disallowSpacesInsideArrayBrackets": true
	,"disallowSpacesInsideParentheses": true
	,"requireSpaceBetweenArguments": true
	,"validateParameterSeparator": ", "
	,"requireSpacesInConditionalExpression": true
	,"requireSpacesInForStatement": true
	,"requireSpacesInsideObjectBrackets": "allButNested"
	,"disallowQuotedKeysInObjects": "allButReserved"
	,"disallowSpaceAfterObjectKeys": true
	,"requireSpaceBeforeObjectValues": true
	,"requireSpaceBeforeBlockStatements": true
	,"requireSpaceAfterLineComment": { "allExcept": ["/", "#", "="] }
	,"requireTrailingComma": { "ignoreSingleLine": true }
	,"requireCommaBeforeLineBreak": true
	,"disallowNewlineBeforeBlockStatements": true
	,"requireBlocksOnNewline": 1
	,"requireOperatorBeforeLineBreak": [
		"?", ":", "+", "/", "*", "%", "=", "+=", "-=", "/=", "*=", "%=", "==", "===", "!=", "!==", ">", ">=", "<", "<=", "&&", "||", "."
	]
	,"disallowSpaceAfterPrefixUnaryOperators": true
	,"disallowSpaceBeforePostfixUnaryOperators": true
	,"requireSpaceBeforeBinaryOperators": true
	,"disallowSpaceBeforeBinaryOperators": [","]
	,"requireSpaceAfterBinaryOperators": true
	,"requireCapitalizedConstructors": true
	,"requireDollarBeforejQueryAssignment": "ignoreProperties"
}
